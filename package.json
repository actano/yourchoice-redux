{
  "name": "yourchoice-redux",
  "version": "0.8.1",
  "description": "Redux wrapper for the yourchoice library",
  "license": "MIT",
  "author": "Actano GmbH (https://rplan.com/)",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/**/*.js",
    "test:watch": "mocha test/**/*.js --watch",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\""
  },
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/actano/yourchoice-redux.git"
  },
  "keywords": [
    "selection",
    "multi",
    "multiselect",
    "range",
    "toggle",
    "shift",
    "iterator",
    "tree",
    "redux"
  ],
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.2.0",
    "codecov": "^1.0.1",
    "cross-env": "^2.0.0",
    "eslint": "^3.2.2",
    "eslint-config-actano": "^2.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "mocha": "^3.0.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "lodash": "^4.5.1",
    "yourchoice": "^2.1.0"
  }
}
